programa 
{                                     
    funcao inicio() {                                       
        inteiro opcao                                       
        leia(opcao)                                         
        logico teste                                        
        leia(teste)                                         
        cadeia testeCadeia = "inicializada"               
        leia(testeCadeia)                                   
        inteiro a, b, c                                     
        leia(a, b, c)                                       
        inteiro vetor[3]                                    
        leia(vetor[0])                                      
        inteiro matriz[3][2]                                
        leia(matriz[0][1])                                  
        leia(matriz[(0+1)%3][1/5+6*2])                                  
    }                                                       
}